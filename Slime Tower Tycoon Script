if game.PlaceId == 10675066724 then
	

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "VIPER HUB", HidePremium = false, IntroText = "VIPER HUB", SaveConfig = true, ConfigFolder = "OrionTest"})

--values



getgenv().oneSlime = true
getgenv().slimeFast = true
getgenv().fiveSlime = true
getgenv().autoMerge = true
getgenv().slowMerge = true
getgenv().autoDep = true
getgenv().depSlow = true
getgenv().buyRate = true
getgenv().slowRate = true
getgenv().autoDrops = true
getgenv().slowDrops = true
getgenv().autoB = true


--functions 

--SLIMES
function oneSlime()
   while  getgenv().oneSlime == true do
local ohNumber1 = 1

game:GetService("ReplicatedStorage").GTycoonClient.Remotes.BuyDropper:FireServer(ohNumber1)
                wait()
   end

end


function slimeFast()
    while  getgenv().slimeFast == true do
 local ohNumber1 = 1
 
 game:GetService("ReplicatedStorage").GTycoonClient.Remotes.BuyDropper:FireServer(ohNumber1)
                 wait(1)
    end
 
 end
 
--SLIMES


--DepositDrops
function autoDep()
    while getgenv().autoDep == true do
game:GetService("ReplicatedStorage").GTycoonClient.Remotes.DepositDrops:FireServer()
                wait()
    end 
end

function depSlow()
    while getgenv().depSlow == true do
game:GetService("ReplicatedStorage").GTycoonClient.Remotes.DepositDrops:FireServer()
                wait(1)
    end 
end

--DepositDrops

--AUTORATE

function buyRate()
    while getgenv().buyRate == true do
local ohNumber1 = 1

game:GetService("ReplicatedStorage").GTycoonClient.Remotes.BuySpeed:FireServer(ohNumber1)
            wait()

    end

end


function slowRate()
    while getgenv().slowRate == true do
local ohNumber1 = 1

game:GetService("ReplicatedStorage").GTycoonClient.Remotes.BuySpeed:FireServer(ohNumber1)
            wait(1)

    end

end
--AUTORATE

--AUTOMERGE
function autoMerge()
    while getgenv().autoMerge == true do
game:GetService("ReplicatedStorage").GTycoonClient.Remotes.MergeDroppers:FireServer()
                wait()
    end

end

function slowMerge()
    while getgenv().slowMerge == true do
game:GetService("ReplicatedStorage").GTycoonClient.Remotes.MergeDroppers:FireServer()
                wait(1)
    end

end
--AUTOMERGE


--AUTODROPS
function autoDrops()
    while getgenv().autoDrops == true do
local ohNumber1 = 1
game:GetService("ReplicatedStorage").GTycoonClient.Remotes.GrabDrops:FireServer(ohNumber1)
            wait()
local ohNumber1 = 4
game:GetService("ReplicatedStorage").GTycoonClient.Remotes.GrabDrops:FireServer(ohNumber1)
            wait()
local ohNumber1 = 18
game:GetService("ReplicatedStorage").GTycoonClient.Remotes.GrabDrops:FireServer(ohNumber1)
            wait()
    end
end






function slowDrops()
    while getgenv().slowDrops == true do
local ohNumber1 = 1
game:GetService("ReplicatedStorage").GTycoonClient.Remotes.GrabDrops:FireServer(ohNumber1)
            wait(0.5)
local ohNumber1 = 4
game:GetService("ReplicatedStorage").GTycoonClient.Remotes.GrabDrops:FireServer(ohNumber1)
            wait(0.5)
local ohNumber1 = 18
game:GetService("ReplicatedStorage").GTycoonClient.Remotes.GrabDrops:FireServer(ohNumber1)
            wait(0.5)
    end
end





function autoB()
    while getgenv().autoB == true do
game:GetService("ReplicatedStorage").GTycoonClient.Remotes.FinishObby:FireServer()
            wait(10)
    end
end





--Toggles/Sections

local Tab = Window:MakeTab({
	Name = "AutoSlimes",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddLabel("AutoSlimes")


Tab:AddToggle({
	Name = "Auto Buy One Slime (QUICK)",
	Default = false,
	Callback = function(Value)
		getgenv().oneSlime = Value
            oneSlime()
	end    
})

Tab:AddToggle({
	Name = "Auto Buy One Slime (SLOW)",
	Default = false,
	Callback = function(Value)
		getgenv().slimeFast = Value
            slimeFast()
	end    
})


Tab:AddLabel("MergeSlimes")

